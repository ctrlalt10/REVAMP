{
  "manifest_version": 3,
  "name": "REVAMPâ„¢",
  "version": "1.0",
  "description": "Personalize your web experience",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "2024_08_20_0sh_Kleki.png",
      "48": "2024_08_20_0sh_Kleki.png",
      "128": "2024_08_20_0sh_Kleki.png"
